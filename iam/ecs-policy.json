{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Effect": "Allow",
      "Action": [
        "ecr:*",
        "logs:CreateLogStream",
        "logs:PutLogEvents",
        "ecs:*",
        "scheduler:*",
        "events:*"
      ],
      "Resource": "*"
    },
    {
      "Effect": "Allow",
      "Action": [
        "iam:PassRole"
      ],
      "Resource": "arn:aws:iam::529875232668:role/ecsTaskexecutionRole"
    }
  ]
}
